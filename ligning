import numpy as np
import math


# Kendte punkter (xy_1, xy_2, xy_3) og distancer (d_1, d_2, d_3)
points = [(x1, y1), (x2, y2), (x3, y3)] # indsæt værdier her
distances = [d1, d2, d3] # indsæt værdier

# ligningen
def equations(x, y):
    

    (math.sqrt((x - points[0][0])**2 + (y - points[0][1])**2) - distances[0])**2
    (math.sqrt((x - points[1][0])**2 + (y - points[1][1])**2) - distances[1])**2
    (math.sqrt((x - points[2][0])**2 + (y - points[2][1])**2) - distances[2])**2
